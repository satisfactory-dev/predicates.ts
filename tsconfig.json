{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"erasableSyntaxOnly": true,
		"noEmit": true,
		"noEmitHelpers": true,
		"sourceMap": true,
		"newLine": "lf",
		"esModuleInterop": true,
		"strict": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"lib": ["esnext"]
}
